                                positioner: function (labelWidth, labelHeight, point) {

                                    var coords = {x: point.plotX - 228, y: point.plotY - 380};
                                    return coords;
                                },
                                shape: 'square',
                                useHTML: true,
                                backgroundColor: null,
                                borderWidth: 0,
                                shadow: false,
                                formatter: function () {

                                    return renderTooltip(this);
                                },
                            },
                            legend: {
                                enabled: false
                            },
                            mapNavigation: {
                                enabled: false
                            },
                            exporting: {
                                enabled: false
                            },
                            credits: false,
                            chart: {
                            },
                            title: {text: '' // Россия
                            },
                            colors: ["#B1806C"],
                            plotOptions: {
                                map: {
                                    states: {
                                        hover: {
                                            color: {
                                                linearGradient: {x1: 0, x2: 0, y1: 0, y2: 1},
                                                stops: [
                                                    [0, '#6ec5b5'],
                                                    [1, '#e3e6ae']
                                                ]
                                            },
                                        }
                                    }
