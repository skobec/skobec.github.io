                            var html = "";
                            html += "                         <div class=\"allocation__popup\">";
                            html += "                            <div class=\"allocation__popup-item allocation__popup-item_first\">";
                            html += "                                <div class=\"allocation__popup-title\">";
                            html += "                                    " + data.point.title + "";
                            html += "                                <\/div>";
                            html += "                                <div class=\"allocation__popup-title allocation__popup-title_right\">";
                            html += "                                    <div class=\"allocation__popup-text\">";
                            html += "                                        Средства из бюджета <br> минпромторга";
                            html += "                                    <\/div>";
                            html += "                                    <div class=\"allocation__popup-count\">";
                            html += "                                        <span class=\"allocation__popup-number\">" + data.point.value + "<\/span> млрд" + "<span class='rouble'> Р</span>";
