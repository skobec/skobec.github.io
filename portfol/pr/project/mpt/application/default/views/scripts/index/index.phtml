                <script>
                    $(document).ready(function () {
                        $('#districts div[role="option"]').on('click', function () {
                            var district = ($(this).data('val'));
                            if (district === 0) {
                                $('#region').hide();
                            } else {
                                var regions = $.grep(<?= json_encode($this->regions) ?>, function (el) {
                                    return el.parent_id === district;
                                });
                                var container = $('#region div[role="list-box"] div');
                                container.empty();
                                $('#region div[role="button"]').html('Все регионы');
                                var row = $('<div>').attr({
                                    'data-val': 0,
                                    'tabindex': 1,
                                    'role': 'option',
                                    'class': 'jelect-option js-jelect-option'
                                }).appendTo(container);

                                var region_text = $('<div>').attr({
                                    'class': 'jelect-option__value js-jelect-option-value'
                                }).html('Все регионы').appendTo(row);
                                $.each(regions, function (key, region) {

                                    var row = $('<div>').attr({
                                        'data-val': region.id,
                                        'tabindex': 1,
                                        'role': 'option',
                                        'class': 'jelect-option js-jelect-option'
                                    }).appendTo(container);

                                    var region_text = $('<div>').attr({
                                        'class': 'jelect-option__value js-jelect-option-value'
                                    }).html(region.title).appendTo(row);

                                });
                                $('#region').show();
                            }


                        });


                        $('#main-map-container').highcharts('Map', {
